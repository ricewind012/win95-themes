[class*="library_"] {
	/{attr(LibraryWindowDivider)} {
		background-color: transparent;
		width: 8px;
	}

	/{attr(MainPanel)} {
		--tab-height: 18px;
		margin-top: var(--tab-height);
	}

	/{attr(AppDetailsTransitionGroup)} {
		@extend [role="tabpanel"];
	}

	/{attr(AppDetailsMain)} {
		@extend table;

		padding: 2px;

		top: 3px;
		right: 4px;
		bottom: 4px;
		left: 3px;
	}
}
