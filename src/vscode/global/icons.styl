.codicon {
	/*
	 * Single icon for multiple classes,
	 * not alphabetically sorted.
	 */
	&-chevron-left,
	&-inline-suggestion-hints-previous {
		--icon-img: var(--icon-arrow-left);
		--button-img: var(--icon-img);
		--button-img-size: 4px 7px;
		--button-img-pos: 4px center;
	}

	&-chevron-up,
	&-parameter-hints-previous,
	&-notifications-expand,
	&-review-comment-collapse,
	&-markers-view-multi-line-expanded,
	&-panel-maximize {
		--icon-img: var(--icon-arrow-top);
		--button-img: var(--icon-img);
		--button-img-size: 7px 4px;
		--button-img-pos: 4px center;
	}

	&-chevron-right,
	&-menu-submenu,
	&-suggest-more-info,
	&-find-collapsed,
	&-inline-suggestion-hints-next,
	&-debug-console-evaluation-prompt,
	&-notebook-collapsed,
	&-search-hide-replace,
	&-view-pane-container-collapsed,
	&-breadcrumb-separator {
		--icon-img: var(--icon-arrow-right);
		--button-img: var(--icon-img);
		--button-img-size: 4px 7px;
		--button-img-pos: 4px center;
	}
	&-folding-collapsed,
	&-folding-manual-collapsed {
		--icon-img: var(--icon-arrow-right);
		replace-icon(4px, 7px);
	}

	&-chevron-down,
	&-drop-down-button,
	&-tree-item-expanded,
	&-parameter-hints-next,
	&-find-expanded,
	&-notifications-hide,
	&-notifications-collapse,
	&-notebook-expanded,
	&-search-show-replace,
	&-markers-view-multi-line-collapsed,
	&-view-pane-container-expanded,
	&-panel-restore {
		--icon-img: var(--icon-arrow-bottom);
		--button-img: var(--icon-img);
		--button-img-size: 7px 4px;
		--button-img-pos: 4px center;
	}
	&-folding-expanded,
	&-folding-manual-expanded {
		--icon-img: var(--icon-arrow-bottom);
		replace-icon(7px, 4px);
	}

	&-add,
	&-plus,
	&-gist-new,
	&-repo-create,
	&-diff-review-insert,
	&-diff-insert,
	&-watch-expressions-add,
	&-watch-expressions-add-function-breakpoint,
	&-keybindings-add,
	&-ports-forward-icon,
	&-terminal-new {
		--button-img: url('data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciIHZpZXdCb3g9IjAgMCA5IDkiPjxyZWN0IHdpZHRoPSI5IiBoZWlnaHQ9IjEiIHk9IjQiLz48cmVjdCB3aWR0aD0iMSIgaGVpZ2h0PSI5IiB4PSI0Ii8+PC9zdmc+Cg==');
		--button-img-size: 9px;
		--button-img-pos: 3px 2px;
	}

	&-alert,
	&-warning,
	&-extensions-warning-message,
	&-markers-view-icon {
		--icon-img: url('data:image/svg+xml;base64,');
		--icon-img-pos: 0 0;
		replace-icon(9px);
	}

	&-book,
	&-remote-explorer-documentation,
	&-open-editors-view-icon {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 11px 10px;
		--button-img-pos: 2px;
	}

	&-clear-all,
	&-debug-console-clear-all,
	&-extensions-clear-search-results,
	&-notebook-clear,
	&-notifications-clear-all,
	&-preferences-clear-input,
	&-search-clear-results {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 9px;
		--button-img-pos: 3px 2px;
	}

	&-notifications-hide,
	&-notifications-collapse,

	&-close,
	&-chrome-close,
	&-dialog-close,
	&-diff-review-close,
	&-notifications-clear,
	&-panel-close,
	&-ports-stop-forward-icon,
	&-remove-close,
	&-search-remove,
	&-settings-remove,
	&-tasks-remove,
	&-terminal-command-history-remove,
	&-tree-filter-clear,
	&-watch-expression-remove,
	&-widget-close,
	&-workspace-trust-editor-cross,
	&-workspace-trust-editor-remove-folder,
	&-x {
		--button-img: var(--icon-close);
		--button-img-size: 8px 7px;
		--button-img-pos: 4px 3px;
	}

	&-collapse-all,
	&-debug-collapse-all,
	&-search-collapse-results {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 9px;
		--button-img-pos: 3px 2px;
	}

	&-error,
	&-stop,
	&-extensions-error-message,
	&-notebook-state-error,
	&-testing-cancel-refresh-tests,
	&-testing-failed-icon {
		--icon-img: url('data:image/svg+xml;base64,');
		--icon-img-pos: 0 0;
		replace-icon(10px);
	}

	&-symbol-reference,
	&-go-to-file,
	&-preferences-open-settings,
	&-search-open-in-file {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 8px 10px;
		--button-img-pos: 4px 1px;
	}

	&-filter,
	&-extensions-filter,
	&-find-filter,
	&-preferences-filter,
	&-testing-filter,
	&-timeline-filter {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 9px;
		--button-img-pos: 3px 2px;
	}

	&-gist-private,
	&-git-fork-private,
	&-lock,
	&-mirror-private,
	&-private-ports-view-icon {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 9px;
		--button-img-pos: 3px 2px;
	}

	// Everything, except the last one, are different, but look similiar
	&-list-selection,
	&-selection,
	&-tree-filter-on-type-off,
	&-find-selection,
	&-search-show-context,
	&-list-flat {
		--button-img: url('data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciIHZpZXdCb3g9IjAgMCA5IDkiPjxyZWN0IHdpZHRoPSI4IiBoZWlnaHQ9IjEiIHk9IjEiLz48cmVjdCB3aWR0aD0iNyIgaGVpZ2h0PSIxIiB5PSIzIi8+PHJlY3Qgd2lkdGg9IjYiIGhlaWdodD0iMSIgeT0iNSIvPjxyZWN0IHdpZHRoPSI1IiBoZWlnaHQ9IjEiIHk9IjciLz48L3N2Zz4K');
		--button-img-size: 9px;
		--button-img-pos: 4px 2px;
	}

	&-refresh,
	&-extensions-refresh,
	&-search-refresh,
	&-testing-rerun-icon,
	&-testing-refresh-tests,
	&-timeline-refresh {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 9px;
		--button-img-pos: 3px 2px;
	}

	&&-gear,
	&&-notifications-configure,
	&&-debug-configure,
	&&-extensions-manage,
	&&-settings-gear,
	&&-settings-view-bar-icon,
	&&-settings-more-action,
	&&-terminal-rename,
	&&-terminal-configure-profile,
	&&-testing-update-profiles,
	&&-tasks-list-configure,
	&&-theme-selection-manage-extension {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 11px;
		--button-img-pos: 2px;

		&::before {
			height: 13px;
		}
	}

	&-trash,
	&-trashcan,
	&-debug-remove-config,
	&-notebook-delete-cell,
	&-terminal-kill {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 9px;
		--button-img-pos: 3px 2px;
	}

	&-more,
	&-ellipsis,
	&-kebab-horizontal,
	&-menubar-more,
	&-toolbar-more,
	&-search-details {
		--button-img: url('data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciIHZpZXdCb3g9IjAgMCA4IDIiPjxzdHlsZT5yZWN0e3dpZHRoOjJweDtoZWlnaHQ6MnB4fTwvc3R5bGU+PHJlY3QgeD0iMCIvPjxyZWN0IHg9IjMiLz48cmVjdCB4PSI2Ii8+PC9zdmc+Cg==');
		--button-img-size: 8px auto;
	}
	/* end */

	/* Dialogs/notifications. */
	.notification-toast &-error {
		background-image: var(--icon-dialog-error);
	}

	&-info,
	&-shield,
	&-dialog-warning {
		background-image: var(--icon-dialog-warning);
	}

	&-dialog-info {
		background-image: var(--icon-dialog-question);
	}
	/* end */

	&-arrow {
		&-left,
		&-right {
			--button-img-size: 4px 7px;
			--button-img-pos: 5px 3px;
		}

		&-left {
			--button-img: var(--icon-arrow-left);
		}

		&-right {
			--button-img: var(--icon-arrow-right);
		}
	}

	&-auxiliarybar-right {
		&-layout-icon {
			--button-img: url('data:image/svg+xml;base64,');
			--button-img-size: 9px;
		}

		&-off-layout-icon {
			--button-img: url('data:image/svg+xml;base64,');
			--button-img-size: 9px;
		}
	}

	&-bell {
		--icon-img: url('data:image/svg+xml;base64,');
		--button-img: var(--icon-img);
		replace-icon(9px);
	}

	&-chrome {
		&-maximize {
			--button-img: var(--icon-maximize);
			--button-img-size: 9px;
			--button-img-pos: 3px 2px;
		}

		&-minimize {
			--button-img: var(--icon-minimize);
			--button-img-size: 6px 2px;
			--button-img-pos: 4px 9px;
		}

		&-restore {
			--button-img: var(--icon-restore);
			--button-img-size: 10px 9px;
			--button-img-pos: 2px 2px;
		}
	}

	&-configure-layout-icon {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 9px;
	}

	&-case-sensitive {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 11px 7px;
		--button-img-pos: 2px 3px;
	}

	&-list-tree {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 9px;
		--button-img-pos: 3px 2px;
	}

	&-menu-submenu {
		--icon-img: var(--icon-arrow-right);
		--button-img: var(--icon-img);
		replace-icon(4px, 7px);

		.action-menu-item:hover > & {
			--color-button-text: var(--color-highlight-text);
		}
	}

	&&-new-file,
	&&-file-add,
	&&-search-new-editor {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 8px 10px;
		--button-img-pos: 4px 2px;
	}

	&&-new-folder {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 11px 10px;
		--button-img-pos: 2px 2px;
	}

	&-panel {
		&-layout-icon {
			--button-img: url('data:image/svg+xml;base64,');
			--button-img-size: 9px;

			&-off {
				--button-img: url('data:image/svg+xml;base64,');
				--button-img-size: 9px;
			}
		}

		&-left {
			--button-img: url('data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciIHZpZXdCb3g9IjAgMCA5IDkiPjxyZWN0IHdpZHRoPSI0IiBoZWlnaHQ9IjkiLz48cmVjdCB3aWR0aD0iNSIgaGVpZ2h0PSIxIiB4PSI0Ii8+PHJlY3Qgd2lkdGg9IjUiIGhlaWdodD0iMSIgeD0iNCIgeT0iOCIvPjxyZWN0IHdpZHRoPSIxIiBoZWlnaHQ9IjciIHg9IjgiIHk9IjEiLz48L3N2Zz4K');
			--button-img-size: 9px;

			&-off {
				--button-img: url('data:image/svg+xml;base64,');
				--button-img-size: 9px;
			}
		}

		&-maximize,
		&-restore {
			--button-img-size: 7px 4px;
			--button-img-pos: 4px center;
		}

		&-maximize {
			--button-img: var(--icon-arrow-top);
		}

		&-restore {
			--button-img: var(--icon-arrow-bottom);
		}
	}

	&-preserve-case {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 11px 7px;
		--button-img-pos: 2px 3px;
	}

	&-regex {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 7px 8px;
		--button-img-pos: 4px 3px;
	}

	&-search-list {
		--button-img: url('data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciIHZpZXdCb3g9IjAgMCA4IDgiPjxzdHlsZT5yZWN0e2hlaWdodDoxcHh9PC9zdHlsZT48cmVjdCB3aWR0aD0iNiIvPjxyZWN0IHdpZHRoPSI4IiB5PSIyIi8+PHJlY3Qgd2lkdGg9IjQiIHk9IjQiLz48cmVjdCB3aWR0aD0iMyIgeT0iNiIvPjwvc3ZnPgo=');
		--button-img-size: 10px 8px;
		--button-img-pos: 3px;
	}

	&-split-horizontal {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 9px;
		--button-img-pos: 3px 2px;
	}

	&-whole-word {
		--button-img: url('data:image/svg+xml;base64,');
		--button-img-size: 11px;
		--button-img-pos: 2px 2px;
	}
}

/* Large icons on the side ... */
.activitybar .codicon {
	&-explorer-view-icon {
		background-image: url('data:image/png;base64,');
	}

	&-search-view-icon {
		background-image: url('data:image/png;base64,');
	}

	&-source-control-view-icon {
		background-image: url('data:image/png;base64,');
	}

	&-run-view-icon {
		background-image: url('data:image/png;base64,');
	}

	&-extensions-view-icon {
		background-image: url('data:image/png;base64,');
	}

	&-settings-view-bar-icon {
		background-image: url('data:image/png;base64,');
	}
}

/* ... and small icons for tabs. */
