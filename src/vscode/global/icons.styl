	}

	&-search-view-icon {
		background-image: url('data:image/png;base64,');
	}

	&-source-control-view-icon {
		background-image: url('data:image/png;base64,');
	}

	&-run-view-icon {
		background-image: url('data:image/png;base64,');
	}

	&-extensions-view-icon {
		background-image: url('data:image/png;base64,');
	}
}
