.codicon {
	/*
	 * Single icon for multiple classes,
	 * not alphabetically sorted.
	 */
	&-chevron-left,
	&-inline-suggestion-hints-previous {
		--icon-img: var(--icon-arrow-left);
		--button-img: var(--icon-img);
		--button-img-size: 4px 7px;
		--button-img-pos: 4px center;
	}

	&-chevron-up,
	&-parameter-hints-previous,
	&-notifications-expand,
	&-review-comment-collapse,
	&-markers-view-multi-line-expanded,
	&-panel-maximize {
		--icon-img: var(--icon-arrow-top);
		--button-img: var(--icon-img);
		--button-img-size: 7px 4px;
		--button-img-pos: 4px center;
	}

	&-chevron-right,
	&-menu-submenu,
	&-suggest-more-info,
	&-find-collapsed,
	&-inline-suggestion-hints-next,
	&-debug-console-evaluation-prompt,
	&-notebook-collapsed,
	&-search-hide-replace,
	&-view-pane-container-collapsed,
	&-breadcrumb-separator {
		--icon-img: var(--icon-arrow-right);
		--button-img: var(--icon-img);
		--button-img-size: 4px 7px;
		--button-img-pos: 4px center;
	}
	&-folding-collapsed,
	&-folding-manual-collapsed {
		--icon-img: var(--icon-arrow-right);
		replace-icon(4px, 7px);
	}

	&-chevron-down,
	&-drop-down-button,
	&-tree-item-expanded,
	&-parameter-hints-next,
	&-find-expanded,
	&-notifications-hide,
	&-notifications-collapse,
	&-notebook-expanded,
	&-search-show-replace,
	&-markers-view-multi-line-collapsed,
	&-view-pane-container-expanded,
	&-panel-restore {
		--icon-img: var(--icon-arrow-bottom);
		--button-img: var(--icon-img);
		--button-img-size: 7px 4px;
		--button-img-pos: 4px center;
	}
	&-folding-expanded,
	&-folding-manual-expanded {
		--icon-img: var(--icon-arrow-bottom);
		replace-icon(7px, 4px);
	}

	&-add,
	&-plus,
	&-gist-new,
	&-repo-create,
	&-diff-review-insert,
	&-diff-insert,
	&-watch-expressions-add,
	&-watch-expressions-add-function-breakpoint,
	&-keybindings-add,
	&-ports-forward-icon,
	&-terminal-new {
		@extend $icon-plus;
	}

	&-alert,
	&-warning,
	&-extensions-warning-message,
	&-markers-view-icon {
		@extend $icon-warning;

		replace-icon(9px);
	}

	&-book,
	&-remote-explorer-documentation,
	&-open-editors-view-icon {
		@extend $icon-book;
	}

	&-clear-all,
	&-debug-console-clear-all,
	&-extensions-clear-search-results,
	&-notebook-clear,
	&-notifications-clear-all,
	&-preferences-clear-input,
	&-search-clear-results {
		@extend $icon-clear;
	}

	&-notifications-hide,
	&-notifications-collapse,

	&-close,
	.titlebar &-chrome-close,
	&-dialog-close,
	&-diff-review-close,
	&-notifications-clear,
	&-panel-close,
	&-ports-stop-forward-icon,
	&-remove-close,
	&-search-remove,
	&-settings-remove,
	&-tasks-remove,
	&-terminal-command-history-remove,
	&-tree-filter-clear,
	&-watch-expression-remove,
	&-widget-close,
	&-workspace-trust-editor-cross,
	&-workspace-trust-editor-remove-folder,
	&-x {
		@extend $titlebar-button-close;
	}

	&-collapse-all,
	&-debug-collapse-all,
	&-search-collapse-results {
		@extend $icon-collapse;
	}

	&-error,
	&-stop,
	&-extensions-error-message,
	&-notebook-state-error,
	&-testing-cancel-refresh-tests,
	&-testing-failed-icon {
		@extend $icon-error;

		replace-icon(10px);
	}

	&-symbol-reference,
	&-go-to-file,
	&-preferences-open-settings,
	&-search-open-in-file {
		@extend $icon-go-to-file;
	}

	&-filter,
	&-extensions-filter,
	&-find-filter,
	&-preferences-filter,
	&-testing-filter,
	&-timeline-filter {
		@extend $icon-filter;
	}

	&-gist-private,
	&-git-fork-private,
	&-lock,
	&-mirror-private,
	&-private-ports-view-icon {
		@extend $icon-lock;
	}

	// Everything, except the last one, are different, but look similiar
	&-list-selection,
	&-selection,
	&-tree-filter-on-type-off,
	&-find-selection,
	&-search-show-context,
	&-list-flat {
		@extend $icon-list;
	}

	&-refresh,
	&-extensions-refresh,
	&-search-refresh,
	&-testing-rerun-icon,
	&-testing-refresh-tests,
	&-timeline-refresh {
		@extend $icon-refresh;
	}

	&&-gear,
	&&-notifications-configure,
	&&-debug-configure,
	&&-extensions-manage,
	&&-settings-gear,
	&&-settings-view-bar-icon,
	&&-settings-more-action,
	&&-terminal-rename,
	&&-terminal-configure-profile,
	&&-testing-update-profiles,
	&&-tasks-list-configure,
	&&-theme-selection-manage-extension {
		@extend $icon-settings;

		&::before {
			height: 13px;
		}
	}

	&-discard,
	&-trash,
	&-trashcan,
	&-debug-remove-config,
	&-notebook-delete-cell,
	&-terminal-kill {
		@extend $icon-trash;
	}

	&-more,
	&-ellipsis,
	&-kebab-horizontal,
	&-menubar-more,
	&-toolbar-more,
	&-search-details {
		@extend $icon-more;
	}
	/* end */

	/* Dialogs */
	&-info,
	&-shield,
	&-dialog-warning {
		background-image: var(--icon-dialog-warning);
	}

	&-dialog-info {
		background-image: var(--icon-dialog-question);
	}
	/* end */

	&-arrow {
		&-left,
		&-right {
			--button-img-size: 4px 7px;
			--button-img-pos: 5px 3px;
		}

		&-left {
			--button-img: var(--icon-arrow-left);
		}

		&-right {
			--button-img: var(--icon-arrow-right);
		}
	}

	&-auxiliarybar-right {
		&-layout-icon {
			@extend $icon-sidebar-right-on;
		}

		&-off-layout-icon {
			@extend $icon-sidebar-right-off;
		}
	}

	&-bell {
		@extend $icon-bell;

		--icon-img: var(--button-img);
		replace-icon(9px);
	}

	&-chrome {
		&-maximize {
			@extend $titlebar-button-maximize;
		}

		&-minimize {
			@extend $titlebar-button-minimize;
		}

		&-restore {
			@extend $titlebar-button-restore;
		}
	}

	&-configure-layout-icon {
		@extend $icon-layout;
	}

	&-case-sensitive {
		@extend $icon-case-sensitive;
	}

	&-list-tree {
		@extend $icon-tree;
	}

	&-menu-submenu {
		--icon-img: var(--icon-arrow-right);
		--button-img: var(--icon-img);
		replace-icon(4px, 7px);

		.action-menu-item:hover > & {
			--color-button-text: var(--color-highlight-text);
		}
	}

	&&-new-file,
	&&-file-add,
	&&-search-new-editor {
		@extend $icon-new-file;
	}

	&&-new-folder {
		@extend $icon-new-folder;
	}

	&-panel {
		&-layout-icon {
			@extend $icon-panel-on;

			&-off {
				@extend $icon-panel-off;
			}
		}

		&-left {
			@extend $icon-panel-left-on;

			&-off {
				@extend $icon-panel-left-off;
			}
		}

		&-maximize,
		&-restore {
			--button-img-size: 7px 4px;
			--button-img-pos: 4px center;
		}

		&-maximize {
			--button-img: var(--icon-arrow-top);
		}

		&-restore {
			--button-img: var(--icon-arrow-bottom);
		}
	}

	&-preserve-case {
		@extend $icon-match-case;
	}

	&-regex {
		@extend $icon-regex;
	}

	&-search-list {
		@extend $icon-list;
	}

	&-split-horizontal {
		@extend $icon-split-horizontal;
	}

	&-whole-word {
		@extend $icon-whole-word;
	}
}

