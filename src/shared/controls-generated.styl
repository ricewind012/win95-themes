/*
 * Drawing pixel by pixel, because:
 * - -webkit-mask relies on background-color
 * - <select> and ::-webkit-scrollbar can not have pseudo-elements
 */
:root {
	--tab-border-left:
		linear-gradient(90deg, var(--color-button-face)        1px, var(--color-button-face)),
		linear-gradient(90deg, var(--color-button-face)        1px, var(--color-button-face)),
		linear-gradient(90deg, var(--color-button-highlight)   1px, var(--color-button-highlight)),
		linear-gradient(90deg, var(--color-button-face)        1px, var(--color-button-face)),
		linear-gradient(90deg, var(--color-button-highlight)   1px, var(--color-button-highlight)),
		linear-gradient(90deg, var(--color-button-face)        1px, var(--color-button-face));

	--tab-border-right:
		linear-gradient(90deg, var(--color-button-shadow)      1px, var(--color-button-shadow)),
		linear-gradient(90deg, var(--color-button-face)        1px, var(--color-button-face)),
		linear-gradient(90deg, var(--color-button-face)        1px, var(--color-button-face)),
		linear-gradient(90deg, var(--color-button-face)        1px, var(--color-button-face)),
		linear-gradient(90deg, var(--color-button-shadow-dark) 1px, var(--color-button-shadow-dark)),
		linear-gradient(90deg, var(--color-button-face)        1px, var(--color-button-face));
}

